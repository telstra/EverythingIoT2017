String sendSheepTalkJsonPayload(const char *image, int id){
  String jsonStart = "{\"Id\":\"" + String(id) + "\",\"Image\":\"";
  String jsonEnd = "\"}";
  
  int imageLength = strlen(image);
  
  contentLength = jsonStart.length();
  contentLength += jsonEnd.length();
  contentLength += imageLength;
  
  int startChar = 0;
  const int segmentLength = 10;
  
  hub.sendBegin(contentLength);
  hub.sendSegment(jsonStart);
  
  // now stream the image in segmentLength byte chunks
  while (startChar + segmentLength < imageLength)
  {
    data = charToString(image, startChar, segmentLength);
    hub.sendSegment(data);
    startChar += segmentLength;
  }
  
  // send remaining bytes in image
  data = charToString(image, startChar, imageLength - startChar);
  hub.sendSegment(data);
  
  hub.sendSegment(jsonEnd);  // send closing json
  
  return hub.sendEnd();
}

String charToString(const char *data, int start, int length)
{
  String result;
  for (int i = 0; i < length; i++)
  {
    result += data[start + i];
  }
  return result;
}
